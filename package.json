{
  "name": "ng-in-viewport",
  "version": "11.0.0",
  "private": true,
  "scripts": {
    "build": "ng build",
    "build:demo": "ng build demo --prod",
    "build:lib": "ng build ng-in-viewport --prod",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate",
    "predocs": "rimraf ./docs",
    "docs": "typedoc --tsconfig ./projects/ng-in-viewport/tsconfig.lib.json --out ./docs/ ./projects/ng-in-viewport/src/index.ts && echo \"\" > ./docs/.nojekyll",
    "e2e:demo:open": "cypress open -P ./projects/demo-e2e",
    "e2e:demo:run": "cypress run -P ./projects/demo-e2e",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "postinstall": "is-ci || husky install",
    "lint": "ng lint && yarn run lint:demo-e2e",
    "lint:demo": "ng lint demo",
    "lint:demo-e2e": "eslint \"projects/demo-e2e/src/**/*.{js,ts}\"",
    "lint:lib": "ng lint ng-in-viewport",
    "ng": "ng",
    "start": "ng serve",
    "test": "jest",
    "test:demo": "jest --config=projects/demo/jest.config.js",
    "test:lib": "jest --config=projects/ng-in-viewport/jest.config.js"
  },
  "dependencies": {
    "@angular/animations": "11.0.9",
    "@angular/common": "11.0.9",
    "@angular/compiler": "11.0.9",
    "@angular/core": "11.0.9",
    "@angular/forms": "11.0.9",
    "@angular/platform-browser": "11.0.9",
    "@angular/platform-browser-dynamic": "11.0.9",
    "@angular/router": "11.0.9",
    "intersection-observer": "0.12.0",
    "rxjs": "6.6.3",
    "tslib": "2.1.0",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1100.7",
    "@angular-eslint/builder": "1.1.0",
    "@angular-eslint/eslint-plugin": "1.1.0",
    "@angular-eslint/eslint-plugin-template": "1.1.0",
    "@angular-eslint/schematics": "1.1.0",
    "@angular-eslint/template-parser": "1.1.0",
    "@angular/cli": "11.0.7",
    "@angular/compiler-cli": "11.0.9",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@types/cypress": "1.1.3",
    "@types/jest": "26.0.19",
    "@types/node": "14.14.21",
    "@typescript-eslint/eslint-plugin": "4.13.0",
    "@typescript-eslint/parser": "4.13.0",
    "all-contributors-cli": "6.19.0",
    "codecov": "3.8.1",
    "conventional-changelog-cli": "2.1.1",
    "cypress": "6.1.0",
    "eslint": "7.18.0",
    "eslint-plugin-cypress": "2.11.2",
    "eslint-plugin-header": "3.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "31.0.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "husky": "5.0.4",
    "is-ci": "2.0.0",
    "jest": "26.6.3",
    "jest-preset-angular": "9.0.0-next.3",
    "lint-staged": "10.5.3",
    "markdownlint-cli2": "0.0.13",
    "ng-packagr": "11.0.3",
    "prettier": "2.2.1",
    "prettier-plugin-organize-imports": "1.1.1",
    "prettier-plugin-packagejson": "2.2.9",
    "rimraf": "3.0.2",
    "ts-node": "9.1.1",
    "typedoc": "0.20.16",
    "typescript": "4.0.5"
  },
  "engines": {
    "node": ">=14.0.0 <16.0.0",
    "npm": "please-use-yarn",
    "yarn": "1.22.10"
  }
}
