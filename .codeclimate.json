{
  "version": "2",
  "plugins": {
    "fixme": {
      "enabled": true
    },
    "markdownlint": {
      "enabled": true
    },
    "scss-lint": {
      "enabled": true
    },
    "eslint": {
      "enabled": true
    }
  },
  "exclude_patterns": [
    "config/",
    "db/",
    "docs/",
    "dist/",
    "features/",
    ".yarn/",
    "**/node_modules/",
    "script/",
    "**/spec/",
    "**/test/",
    "**/tests/",
    "Tests/",
    "**/vendor/",
    "**/*_test.go",
    "**/test/",
    "**/mocks/",
    "**/*.spec.ts",
    "**/*.po.ts"
  ]
}
